{
    "name": "hassnainabass/UsersPermissions",
    "description": "UsersPermissions plugin for CakePHP with GUI",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "UsersPermissions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UsersPermissions\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
